    <rules>
        <fullName>SetHTDiv2TSRemind</fullName>
        <active>true</active>
        <booleanFilter>1 AND (2 OR 3) AND 4 AND 5</booleanFilter>
        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_DIV2__c</field>
            <operation>greaterThan</operation>
            <value>&quot; &quot;</value>
        </criteriaItems>
        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_DIV2_Action__c</field>
            <operation>contains</operation>
            <value>Testimony_Comment,Testimony_Oppose,Testimony_Support</value>
        </criteriaItems>
        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_DIV2_Hearing_Position__c</field>
            <operation>contains</operation>
            <value>Testimony_Comment,Testimony_Oppose,Testimony_Support</value>
        </criteriaItems>
        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_DIV2_Testimony_Status__c</field>
            <operation>notEqual</operation>
            <value>Submitted</value>
        </criteriaItems>
        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_Frmtd_Date__c</field>
            <operation>greaterOrEqual</operation>
            <value>NOW()</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Alert_Testimony_DIV2_due_Reminder</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_Mtg_Adjusted__c</offsetFromField>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>SetHTDiv3TSRemind</fullName>
