        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_DIV3__c</field>
            <operation>greaterThan</operation>
            <value>&quot; &quot;</value>
        </criteriaItems>
        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_DIV3_Action__c</field>
            <operation>contains</operation>
            <value>Testimony_Comment,Testimony_Oppose,Testimony_Support</value>
        </criteriaItems>
        <criteriaItems>
            <field>DCCA_LeTS_Hearings_Testimony__c.DCCA_LeTS_HT_DIV3_Hearing_Position__c</field>
            <operation>contains</operation>
            <value>Testimony_Comment,Testimony_Oppose,Testimony_Support</value>
